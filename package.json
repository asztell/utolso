{
  "author": "ELC Online",
  "description": "The Web Starter Template (WST) is a starting point for creating Front End Web applications.",
  "license": "All rights reserved ELC Online",
  "main": "app.js",
  "name": "web-starter-template",
  "type": "module",
  "version": "0.1.0",
  "keywords": [
    "Template"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "dev-server": "cross-env set NODE_ENV=development&&gulp dev_server",
    "globals": "npm i -g cross-env",
    "image-min": "cross-env set NODE_ENV=development&&gulp images",
    "node-server": "cross-env set NODE_ENV=development&&nodemon server/app.js",
    "prod": "cross-env set NODE_ENV=production&&gulp prod",
    "sass": "cross-env set NODE_ENV=development&&gulp sass",
    "servers": "set NODE_ENV=development&&gulp watch",
    "test": "jest",
    "test:watch": "jest --watch --coverage scripts",
    "third-party": "cross-env set NODE_ENV=development&&gulp vendor"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.4.4",
    "web-vitals": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.0.0-beta.36",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@tsconfig/node19": "^2.0.1",
    "@types/jest": "^29.5.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.14",
    "@types/webpack": "^5.28.1",
    "babel-loader": "^9.1.2",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.3",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-git": "^2.5.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-mustache": "3.0.0",
    "gulp-rename": "1.2.2",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "2.6.1",
    "gulp-uglify": "^3.0.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdom-quokka-plugin": "^1.0.16",
    "nodemon": "^1.14.7",
    "react-native-dotenv": "^2.5.5",
    "sass": "^1.57.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "uglify-js": "3.2.2",
    "webpack": "^5.75.0",
    "webpack-stream": "^7"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "<rootDir>/app/scripts/**/*.{ts,tsx}",
      "!<rootDir>/app/scripts/index.tsx"
    ],
    "setupFiles": [
      "./app/scripts/setupTests.ts"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
